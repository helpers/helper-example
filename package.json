{
  "name": "helper-example",
  "description": "Opinionated template helper that inserts a code example from a filepath.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/helper-example",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/helper-example",
  "bugs": {
    "url": "https://github.com/jonschlinkert/helper-example/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "*",
    "should": "*"
  },
  "keywords": [
    "example",
    "helper"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "generate",
        "templates",
        "update",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "templates",
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
